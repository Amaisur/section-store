  @font-face {
  font-family: "Josefin Sans";
  font-weight: 400;
  font-style: normal;
  font-display: swap;
  src: url("//section.store/cdn/fonts/josefin_sans/josefinsans_n4.c8300d95fd4ce72542a6efba9c682da40d144fba.woff2?h1=c2VjdGlvbi5zdG9yZQ&h2=c2VjdGlvbi1zdG9yZS1hcHAuYWNjb3VudC5teXNob3BpZnkuY29t&hmac=1081bc13bac779d73b77022741d8b8608aa5451af6a05907fc023625940c00f1") format("woff2"),
       url("//section.store/cdn/fonts/josefin_sans/josefinsans_n4.ed7230a86e75b34b997bd12a5e1b87fcaf7104d8.woff?h1=c2VjdGlvbi5zdG9yZQ&h2=c2VjdGlvbi1zdG9yZS1hcHAuYWNjb3VudC5teXNob3BpZnkuY29t&hmac=3f19a3e11bab2423756ce410220df09957f15affd53a78fa0ec435e5660aa636") format("woff");
}

  
  .section-{{ section.id }} {
    border-top: solid #000000 0px;
    border-bottom: solid #000000 0px;
    margin-top: 0px;
    margin-bottom: 0px;
    overflow: hidden;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ section.settings.pt }}px;
    padding-bottom: {{ section.settings.pt }}px;
    padding-left: 0rem;
    padding-right: 0rem;
  }

  .announcement-items-{{ section.id }} {
    flex-shrink: 0;
    display: flex;
    align-items: center;
    flex-wrap: nowrap;
    justify-content: flex-start;
    animation: carouselAnnouncementBar infinite linear;
    animation-duration: 40s;
  }

  .announcement-item-{{ section.id }} {
    flex-shrink: 0;
    margin-left: 30px;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    gap: 10px;
  }

  .main-txt-d5.announcement-text-{{ section.id }}  {
    margin: 0;
    font-size: {{ section.settings.fs-t }}px;
    color: {{ section.settings.c-t }};
    line-height: 100%;
    text-transform: unset;
  }
  .small-txt-d5.announcement-text-{{ section.id }}  {
    margin: 0;
    font-size: {{ section.settings.fs-t-s }}px;
    color: {{ section.settings.c-t-s }};
    line-height: 100%;
    text-transform: unset;
  }

  .section-{{ section.id }} a {
    text-decoration:none!important;
  }

  .announcement-icon-{{ section.id }} {
    height: 20px;
    width: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
  }

  .announcement-icon-{{ section.id }} img,
  .announcement-icon-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: content;
  }

  .announcement-icon-{{ section.id }} svg path {
    fill: #ffffff;
  }

  .announcement-inner-{{ section.id }} {
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
  }
  .text-content-d5{
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
  }
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: 0px;
      margin-bottom: 0px;
    }
    
    .section-{{ section.id }}-settings {
      padding-top: {{ section.settings.pt-d }}px;
      padding-bottom:{{ section.settings.pb-d }}px;
      padding-left: 0rem;
      padding-right: 0rem;
    }

    .announcement-item-{{ section.id }} { 
      margin-left: 50px;
    }

    .announcement-icon-{{ section.id }} {
      height: 20px;
      width: 20px;
    }

  
  }</style>




  <style>
    @keyframes carouselAnnouncementBar {
      0% {
          transform: translateX(-100%);
      }
      
      100% {
          transform: translateX(0%);
      }
    }
  </style>



  <style>
    .announcement-inner-{{ section.id }}:hover .announcement-items-{{ section.id }} {
       animation-play-state: paused 
    }
  </style>


