<style>

#shopify-section-{{ section.id }} i,.ss-rating-container i:before {
    width: 64px;
    height: 64px;
    background-repeat: no-repeat !important;
    background-position: 0 0;
    background-size: 64px 64px;
    background-image: url(https://cdn.shopify.com/s/files/1/0562/7763/1105/files/svg-star.svg?v=1732888283);
    display: inline-block;
  }
  #shopify-section-{{ section.id }} i{
    position: relative;
  }
.ss-rating-container i:before {
    content: none;
    display: none;
    position: absolute;
    top: 0;
    left: 0;
    width: var(--partial-width, 0);
}
.ss-rating-container i.ss-partial:before {
    content: "";
    display: block;
}

  
</style>


{% render 'ss-rating-star-v2' %}
<script src="{{ 'ss-rating-star-v2.js' | asset_url }}" defer="defer"></script>

{% schema %}
{
  "name": "Star component",
  "settings": [],
  "presets": [
    {
      "name": "Star component"
    }
  ]
}
{% endschema %}
